<?xml version='1.0' encoding='utf-8'?>
<widget version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <access origin="*" />
  
  <feature name="CallNumber">
    <param name="ios-package" value="CFCallNumber"/>
  </feature>

  <feature name="ThreeDeeTouch">
    <param name="ios-package" value="ThreeDeeTouch"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="Badge">
    <param name="ios-package" value="APPBadge"/>
  </feature>

  <feature name="Chooser">
    <param name="ios-package" value="Chooser"/>
  </feature>

  <feature name="Device">
    <param name="ios-package" value="CDVDevice"/>
  </feature>

  <feature name="File">
    <param name="ios-package" value="CDVFile"/>
    <param name="onload" value="true"/>
  </feature>

  <feature name="LocalNotification">
    <param name="ios-package" value="APPLocalNotification"/>
  </feature>

  <feature name="Capture">
    <param name="ios-package" value="CDVCapture"/>
  </feature>

  <feature name="VideoEditor">
    <param name="ios-package" value="VideoEditor"/>
  </feature>

  
</widget>